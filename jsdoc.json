{
  "tags": {
    "allowUnknownTags": ["category", "optional"]
  },

  "plugins": ["node_modules/better-docs/typescript", "node_modules/better-docs/category"],
  "opts": {
    "template": "node_modules/better-docs/",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "encoding": "utf8"
  },
  "source": {
    "includePattern": "\\.(ts)$",
    "excludePattern": ".*\\.(xml).d.ts",
    "exclude": ["./src/__tests__", "./src/tools"],
    "include": [
      "./src/StudentVue",
      "./src/Constants",
      "./src/StudentVue/RequestException",
      "./src/StudentVue/Message",
      "./src/StudentVue/Icon",
      "./src/StudentVue/Client",
      "./src/StudentVue/Attachment",
      "./src/StudentVue/File",
      "./src/StudentVue/ReportCard",
      "./src/StudentVue/Document",
      "./src/utils/soap/Client/Client.interfaces.d.ts"
    ]
  }
}
