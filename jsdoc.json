{
  "tags": {
    "allowUnknownTags": ["category", "optional"]
  },

  "plugins": ["plugins/markdown", "node_modules/better-docs/typescript", "node_modules/better-docs/category"],
  "opts": {
    "template": "node_modules/better-docs/",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "encoding": "utf8"
  },
  "source": {
    "includePattern": "\\.(js|ts)$",
    "excludePattern": "\\.(xml.d.ts)$",
    "exclude": ["./src/StudentVue/Client/Interfaces/xml"],
    "include": ["./lib", "./src/StudentVue/Client/Interfaces", "./src/StudentVue/Client/Client.interfaces.d.ts"]
  },
  "recurseDepth": 20
}
